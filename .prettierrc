{
  "$schema": "http://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleQuote": false,
  "trailingComma": "none",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.(js|jsx)",
      "options": {
        "singleQuote": true
      }
    },
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
